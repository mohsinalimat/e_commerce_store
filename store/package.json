{
  "name": "store",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "yarn install && vite build --base=/assets/e_commerce_store/store/ && yarn copy-html-entry",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
